<% title "Photo" %>

<p>
  <strong>Name:</strong>
  <%=h @photo.name %>
</p>
<p>
  <strong>Project:</strong>
  <%=h @photo.project_id %>
</p>

<p>
  <%= link_to "Edit", edit_project_photo_path(current_project, @photo) %> |
  <%= link_to "Destroy", [current_project, @photo], :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", project_photos_path(current_project) %>
</p>
